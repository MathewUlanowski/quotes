<h1 class="text-center m-3 mb-5">Here are the awesome quotes</h1>
<div class="row mt-5">
    <div class="col-1"></div>
    <div class="col-10">
        <ul class="list-group list-group-flush">
            @{
                foreach(var quote in ViewBag.quotes){
                    <li class="list-group-item">
                        <p>@quote["quote"]</p>
                        <p class="ml-5"> - @quote["name"] at @quote["created_at"]</p>
                    </li>
                }
            }
        </ul>
    </div>
</div>